{
  "version": 2,
  "name": "piko-cafe-digital-menu",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/assets/(.*\\.js)$",
      "dest": "/assets/$1",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/assets/(.*\\.css)$",
      "dest": "/assets/$1",
      "headers": {
        "Content-Type": "text/css"
      }
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
